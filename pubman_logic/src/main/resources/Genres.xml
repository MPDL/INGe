<?xml version="1.0" encoding="UTF-8"?>
<configuration-settings metadata-profile-version="1" xmlns="http://escidoc.mpg.de/metadataprofile/schema/0.1/publicationform" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<configuration>
		<!-- The listed content-categories will be available in PubMan -->
		<content-categories>
			<content-category id="ANY_FULLTEXT" url="http://purl.org/escidoc/metadata/ves/content-categories/any-fulltext"/>
			<content-category id="PRE_PRINT" url="http://purl.org/escidoc/metadata/ves/content-categories/pre-print"/>
			<content-category id="POST_PRINT" url="http://purl.org/escidoc/metadata/ves/content-categories/post-print"/>
			<content-category id="PUBLISHER_VERSION" url="http://purl.org/escidoc/metadata/ves/content-categories/publisher-version"/>
			<content-category id="ABSTRACT" url="http://purl.org/escidoc/metadata/ves/content-categories/abstract"/>
			<content-category id="TABLE_OF_CONTENTS" url="http://purl.org/escidoc/metadata/ves/content-categories/table-of-contents"/>
			<content-category id="SUPPLEMENTARY_MATERIAL" url="http://purl.org/escidoc/metadata/ves/content-categories/supplementary-material"/>
			<content-category id="CORRESPONDENCE" url="http://purl.org/escidoc/metadata/ves/content-categories/correspondence"/>
			<content-category id="COPYRIGHT_TRANSFER_AGREEMENT" url="http://purl.org/escidoc/metadata/ves/content-categories/copyright-transfer-agreement"/>
			<content-category id="RESEARCH_DATA" url="http://purl.org/escidoc/metadata/ves/content-categories/research-data"/>
			<content-category id="MULTIMEDIA" url="http://purl.org/escidoc/metadata/ves/content-categories/multimedia"/>
			<content-category id="CODE" url="http://purl.org/escidoc/metadata/ves/content-categories/code"/>
		</content-categories>

		<genre-default>
			<group id="item_basic" repeatable="false" display="true" optional="false" label="g_item_basic">
				<field id="item_genre" repeatable="false" display="true" optional="false" label="item_genre" class-attribute="metadata.genre"/>
				<field id="item_title" repeatable="false" display="true" optional="false" label="item_title" class-attribute="metadata.title"/>
				<field id="item_title_alternative" repeatable="true" display="true" optional="true" label="item_title_alternative" class-attribute="metadata.alternativeTitles"/>
			</group>
			<group id="creator_person_organization" repeatable="true" display="true" optional="false" label="g_creator_person_organization">
				<group id="creator" display="true" repeatable="true" optional="false" label="g_creator" default-value="persons" class-attribute="metadata.creators">
					<group id="select_roles" repeatable="false" display="true" optional="false" label="g_select_roles" class-attribute="metadata.creators.role">
						<field id="creator_type" repeatable="false" display="true" optional="false" default-value="author" label="creator_type" class-attribute="metadata.creators.type"/>
					</group>
					<group id="persons" repeatable="true" display="true" optional="true" label="g_persons" class-attribute="metadata.creators.person">
						<field id="person_family-name" display="true" label="person_family-name" optional="false" repeatable="false" class-attribute="metadata.creators.person.familyName"/>
						<field id="person_given-name" display="true" label="person_given-name" optional="true" repeatable="false" class-attribute="metadata.creators.person.givenName"/>
						<field id="person_identifier_type" display="true" label="person_identifier_type" optional="true" repeatable="false" class-attribute="metadata.creators.person.identifier.type"/>
						<field id="person_identifier_value" display="true" label="person_identifier_value" optional="true" repeatable="false" class-attribute="metadata.creators.person.identifier.id"/>
					</group>
					<group id="organizations" repeatable="true" display="true" optional="true" label="g_organizations" class-attribute="metadata.creators.organization">
						<field id="organization_name" display="true" label="organization_name" optional="false" repeatable="false" class-attribute="metadata.creators.organization.name"/>
						<field id="organization_address" display="true" label="organization_address" optional="true" repeatable="false" class-attribute="metadata.creators.organization.address"/>
						<field id="organization_identifier_type" display="true" label="organization_identifier_type" optional="true" repeatable="false" class-attribute="metadata.creators.organization.identifier.type"/>
						<field id="organization_identifier_value" display="true" label="organization_identifier_value" optional="false" repeatable="false" class-attribute="metadata.creators.organization.identifier.id"/>
						<!-- the organization identifier is not optional in case organization name is entered. This is the logic behind the upper setting -->
					</group>
				</group>
			</group>
			<group id="files" display="true" repeatable="true" optional="true" label="g_files" class-attribute="files">
				<field id="component_name" repeatable="false" display="true" label="component_name" optional="false" class-attribute="files.name"/>
				<field id="component_content-category" repeatable="false" display="true" optional="false" label="component_content-category" class-attribute="files.contentCategory"/>
				<field id="component_visibility" repeatable="false" display="true" optional="false" label="component_visibility" class-attribute="files.visibility"/>
				<field id="component_content" repeatable="false" display="true" optional="false" label="component_content" class-attribute="files.content"/>
				<field id="component_description" repeatable="false" display="true" optional="true" label="component_description" class-attribute="files.description"/>
			</group>
			<group id="locators" display="true" repeatable="true" optional="true" label="g_locators" class-attribute="files">
				<field id="component_name" repeatable="false" display="true" label="locator_name" optional="false" class-attribute="files.name"/>
				<field id="component_content-category" repeatable="false" display="true" optional="false" label="locator_content-category" class-attribute="files.contentCategory"/>
				<field id="component_visibility" repeatable="false" display="true" optional="false" label="locator_component_visibility" class-attribute="files.visibility"/>
				<field id="component_description" repeatable="false" display="true" optional="true" label="locator_description" class-attribute="files.description"/>
			</group>
			<group id="content" repeatable="false" display="true" optional="true" label="g_content">
				<field id="item_abstract" repeatable="true" display="true" label="item_abstract" optional="true" class-attribute="metadata.abstracts"/>
				<group id="item_subject" repeatable="true" display="true" label="item_subject" optional="true" class-attribute="metadata.subjects">
					<field id="item_subject_type" repeatable="false" display="true" optional="false" label="item_subject_type" class-attribute="metadata.subjects.type"/>
					<field id="item_subject_value" repeatable="false" display="true" optional="false" label="item_subject_value" class-attribute="metadata.subjects.value"/>
				</group>
			</group>
			<group id="details" repeatable="false" display="true" optional="true" label="g_details">
				<group id="item_details_dates" repeatable="false" display="true" optional="false" label="g_item_details_dates">
					<field id="item_date-issued" repeatable="false" display="true" label="item_date-issued" optional="true" class-attribute="metadata.datePublishedInPrint"/>
					<field id="item_date-accepted" repeatable="false" display="true" optional="true" label="item_date-accepted" class-attribute="metadata.dateAccepted"/>
					<field id="item_date-modified" repeatable="false" display="true" optional="true" label="item_date-modified" class-attribute="metadata.dateModified"/>
					<field id="item_date-created" repeatable="false" display="true" optional="true" label="item_date-created" class-attribute="metadata.dateCreated"/>
					<field id="item_date-submitted" repeatable="false" display="true" optional="true" label="item_date-submitted" class-attribute="metadata.dateSubmitted"/>
					<field id="item_published-online" repeatable="false" display="true" optional="true" label="item_published-online" class-attribute="metadata.datePublishedOnline"/>
				</group>
				<group id="publishing_info" repeatable="false" display="true" label="g_publishing_info" optional="true" class-attribute="metadata.publishingInfo">
					<field id="publishing-info_publisher" repeatable="false" display="true" label="publishing-info_publisher" optional="true" class-attribute="metadata.publishingInfo.publisher"/>
					<field id="publishing-info_place" repeatable="false" display="true" label="publishing-info_place" optional="true" class-attribute="metadata.publishingInfo.place"/>
					<field id="publishing-info_edition" repeatable="false" display="true" optional="true" label="publishing-info_edition" class-attribute="metadata.publishingInfo.edition"/>
				</group>
				<group id="total_number_of_pages" repeatable="false" display="true" optional="true" label="g_total_number_of_pages" class-attribute="metadata.totalNumberOfPages">
					<field id="item_total-number-of-pages" repeatable="false" display="true" label="item_total-number-of-pages" optional="true" class-attribute="metadata.totalNumberOfPages"/>
				</group>
				<group id="identifiers" display="true" optional="true" repeatable="true" label="g_identifiers" class-attribute="metadata.identifiers">
					<field id="item_identifier_type" repeatable="false" display="true" optional="false" label="item_identifier_type" class-attribute="metadata.identifiers.type"/>
					<field id="item_identifier_value" repeatable="false" display="true" optional="false" label="item_identifier_value" class-attribute="metadata.identifiers.id"/>
				</group>
				<group id="review_method" repeatable="false" display="true" optional="true" label="g_review_method" class-attribute="metadata.reviewMethod">
					<field id="item_review-method" repeatable="false" optional="true" display="true" label="item_review-method" class-attribute="metadata.reviewMethod"/>
				</group>
				<group id="item_degree" repeatable="false" display="false" optional="true" label="g_item_degree" class-attribute="metadata.degree">
					<field id="item_degree" repeatable="false" optional="true" display="true" label="item_degree" class-attribute="metadata.degree"/>
				</group>
				<group id="item_language" repeatable="true" display="true" optional="true" label="g_item_language" class-attribute="metadata.languages">
					<field id="item_language" repeatable="true" optional="true" display="true" label="item_language" class-attribute="metadata.languages"/>
				</group>
				<group id="item_toc" repeatable="false" display="true" optional="true" label="g_item_toc" class-attribute="metadata.tableOfContents">
					<field id="item_table-of-contents" repeatable="false" optional="true" display="true" label="item_table-of-contents" class-attribute="metadata.tableOfContents"/>
				</group>
			</group>
			<group id="events" repeatable="false" display="true" optional="true" label="g_events" class-attribute="metadata.event">
				<field id="event_title" repeatable="false" display="true" optional="false" label="event_title" class-attribute="metadata.event.title"/>
				<field id="event_invitation-status" repeatable="false" display="true" optional="true" label="event_invitation-status" class-attribute="metadata.event.invitationStatus"/>
				<field id="event_place" repeatable="false" display="true" optional="true" label="event_place" class-attribute="metadata.event.place"/>
				<field id="event_start-date" repeatable="false" display="true" optional="true" label="event_start-date" class-attribute="metadata.event.startDate"/>
				<field id="event_end-date" repeatable="false" display="true" optional="true" label="event_end-date" class-attribute="metadata.event.endDate"/>
			</group>
			<!-- JUS-specific default configuration legal case BEGIN -->
			<group id="legal_case" repeatable="false" display="false" optional="true" label="g_legal_case" class-attribute="metadata.legalCase">
				<field id="legal_case_court_name" repeatable="false" display="true" optional="true" label="legal_case_court_name" class-attribute="metadata.legalCase.courtName"/>
				<field id="legal_case_identifier" repeatable="false" display="true" optional="true" label="legal_case_identifier" class-attribute="metadata.legalCase.identifier"/>
				<field id="legal_case_title" repeatable="false" display="true" optional="true" label="legal_case_title" class-attribute="metadata.legalCase.title"/>
				<field id="legal_case_date_published" repeatable="false" display="true" optional="true" label="legal_case_date_published" class-attribute="metadata.legalCase.datePublished"/>
			</group>
			<!-- JUS-specific default configuration legal case END -->
			<group id="sources" repeatable="true" display="true" optional="true" label="g_sources" class-attribute="metadata.sources">
				<group id="source_basic" repeatable="false" display="true" optional="false" label="g_source_basic">
					<field id="source_genre" repeatable="false" display="true" optional="false" label="source_genre" class-attribute="metadata.sources.genre"/>
					<field id="source_title" repeatable="false" display="true" optional="false" label="source_title" class-attribute="metadata.sources.title"/>
					<group id="source_title_alternative" repeatable="true" display="true" optional="true" label="source_title_alternative" class-attribute="metadata.sources.alternativeTitles">
						<field id="source_title_alternative_type" repeatable="false" display="true" optional="false" label="source_title_alternative_type" class-attribute="metadata.sources.alternativeTitles.type"/>
						<field id="source_title_alternative_value" repeatable="false" display="true" optional="false" label="source_title_alternative_value" class-attribute="metadata.sources.alternativeTitles.value"/>
					</group>
				</group>
				<group id="source_person_organization" repeatable="true" display="true" optional="true" label="g_source_person_organization" class-attribute="metadata.sources.creators">
					<group id="select_roles" repeatable="false" display="true" optional="true" label="g_select_roles" class-attribute="metadata.sources.creators.role">
						<field id="creator_type" repeatable="false" display="true" optional="true" default-value="author" label="creator_type" class-attribute="metadata.sources.creators.type"/>
					</group>
					<group id="source_persons" repeatable="true" display="true" optional="true" label="g_source_persons" class-attribute="metadata.sources.creators.person">
						<field id="person_family-name" display="true" label="person_family-name" optional="false" repeatable="false" class-attribute="metadata.sources.creators.person.familyName"/>
						<field id="person_given-name" display="true" label="person_given-name" optional="true" repeatable="false" class-attribute="metadata.sources.creators.person.givenName"/>
						<field id="person_identifier_type" display="true" label="person_identifier_type" optional="true" repeatable="false" class-attribute="metadata.sources.creators.person.identifier.type"/>
						<field id="person_identifier_value" display="true" label="person_identifier_value" optional="true" repeatable="false" class-attribute="metadata.sources.creators.person.identifier.id"/>
					</group>
					<group id="source_organizations" repeatable="true" display="true" optional="true" label="g_source_organizations" class-attribute="metadata.sources.creators.organization">
						<field id="organization_name" display="true" label="organization_name" optional="false" repeatable="false" class-attribute="metadata.sources.creators.organization.name"/>
						<field id="organization_address" display="true" label="organization_address" optional="true" repeatable="false" class-attribute="metadata.sources.creators.organization.address"/>
						<field id="organization_identifier_type" display="true" label="organization_identifier_type" optional="true" repeatable="false" class-attribute="metadata.sources.creators.organization.identifier.type"/>
						<field id="organization_identifier_value" display="true" label="organization_identifier_value" optional="false" repeatable="false" class-attribute="metadata.sources.creators.organization.identifier.id"/>
					</group>
				</group>
				<group id="source_publishing_info" repeatable="false" display="true" label="g_source_publishing_info" optional="true" class-attribute="metadata.sources.publishingInfo">
					<field id="publishing-info_publisher" repeatable="false" display="true" label="publishing-info_publisher" optional="true" class-attribute="metadata.sources.publishingInfo.publisher"/>
					<field id="publishing-info_place" repeatable="false" display="true" label="publishing-info_place" optional="true" class-attribute="metadata.sources.publishingInfo.place"/>
					<field id="publishing-info_edition" repeatable="false" display="true" optional="true" label="publishing-info_edition" class-attribute="metadata.sources.publishingInfo.edition"/>
				</group>
				<group id="source_total_number_of_pages" repeatable="false" display="true" optional="true" label="g_source_total_number_of_pages" class-attribute="metadata.sources.totalNumberOfPages">
					<field id="source_total-number-of-pages" repeatable="false" display="true" label="source_total-number-of-pages" optional="true" class-attribute="metadata.sources.totalNumberOfPages"/>
				</group>
				<group id="source_volume_issue_pages" repeatable="false" display="true" optional="true" label="g_source_volume_issue_pages">
					<field id="source_volume" repeatable="false" display="true" optional="true" label="source_volume" class-attribute="metadata.sources.volume"/>
					<field id="source_issue" repeatable="false" display="true" optional="true" label="source_issue" class-attribute="metadata.sources.issue"/>
					<field id="source_start-page" repeatable="false" display="true" optional="true" label="source_start-page" class-attribute="metadata.sources.startPage"/>
					<field id="source_end-page" repeatable="false" display="true" optional="true" label="source_end-page" class-attribute="metadata.sources.endPage"/>
				</group>
				<group id="source_sequence_number" repeatable="false" display="true" optional="true" label="g_source_sequence_number" class-attribute="metadata.sources.sequenceNumber">
					<field id="source_sequence-number" repeatable="false" display="true" optional="true" label="source_sequence_number" class-attribute="metadata.sources.sequenceNumber"/>
				</group>
				<group id="source_identifiers" display="true" optional="true" repeatable="true" label="g_source_identifiers" class-attribute="metadata.sources.identifiers">
					<field id="source_identifier_type" repeatable="false" display="true" optional="false" label="source_identifier_type" class-attribute="metadata.sources.identifiers.type"/>
					<field id="source_identifier_value" repeatable="false" display="true" optional="false" label="source_identifier_value" class-attribute="metadata.sources.identifiers.id"/>
				</group>
			</group>
			<group id="project_info" repeatable="true" display="true" optional="true" label="g_project_info" class-attribute="metadata.projectInfo">
				<field id="title" repeatable="false" display="true" optional="true" label="project_info_title" class-attribute="metadata.projectInfo.title"/>
				<field id="grant_id" repeatable="false" display="true" optional="true" label="project_info_grant_id" class-attribute="metadata.projectInfo.grantIdentifier.id"/>
				<group id="funding_info" repeatable="false" display="true" optional="true" label="g_project_funding_info" class-attribute="metadata.projectInfo.fundingInfo">
					<field id="organization_title" repeatable="false" display="true" optional="true" label="project_info_funding_info_organization_title" class-attribute="metadata.projectInfo.fundingInfo.fundingOrganization.title"/>
					<field id="organization_id" repeatable="false" display="true" optional="true" label="project_info_funding_info_organization_id" class-attribute="metadata.projectInfo.fundingInfo.fundingOrganization.id"/>
					<field id="program_title" repeatable="false" display="true" optional="true" label="project_info_funding_info_program_title" class-attribute="metadata.projectInfo.fundingInfo.fundingProgram.title"/>
					<field id="program_id" repeatable="false" display="true" optional="true" label="project_info_funding_info_program_id" class-attribute="metadata.projectInfo.fundingInfo.fundingProgram.identifiers.id"/>
				</group>
			</group>
		</genre-default>

