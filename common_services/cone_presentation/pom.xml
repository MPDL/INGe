<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<parent>
		<artifactId>common_services</artifactId>
		<groupId>de.mpg.escidoc.services</groupId>
		<version>1.0.6-SNAPSHOT</version>
	</parent>
	<modelVersion>4.0.0</modelVersion>
	<groupId>de.mpg.escidoc.services</groupId>
	<artifactId>cone_presentation</artifactId>
	<packaging>war</packaging>
	<name>cone_presentation</name>
	<version>1.0.2-SNAPSHOT</version>
	<url>http://maven.apache.org</url>
	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-eclipse-plugin</artifactId>
				<configuration>
					<wtpversion>1.5</wtpversion>
				</configuration>
			</plugin>
			<plugin>
				<artifactId>maven-compiler-plugin</artifactId>
				<configuration>
					<source>1.5</source>
					<target>1.5</target>
				</configuration>
			</plugin>
		</plugins>
	</build>
	<profiles>
		<profile>
			<id>jboss42</id>
			<build>
				<plugins>
					<plugin>
						<groupId>org.codehaus.mojo</groupId>
						<artifactId>jboss-maven-plugin</artifactId>
						<configuration>
							<jbossHome>${jboss.install.dir}</jbossHome>
							<hostName>${jboss.deploy.host}</hostName>
							<port>${jboss.deploy.port}</port>
						</configuration>
					</plugin>
				</plugins>
			</build>
		</profile>
	</profiles>
	<dependencies>
		<dependency>
			<groupId>junit</groupId>
			<artifactId>junit</artifactId>
			<version>4.1</version>
			<scope>test</scope>
		</dependency>
		<dependency>
			<groupId>javaee</groupId>
			<artifactId>javaee</artifactId>
			<version>1.5</version>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>org.mulgara</groupId>
			<artifactId>itql</artifactId>
			<version>2.0-beta</version>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>de.mpg.escidoc.services</groupId>
			<artifactId>framework_access</artifactId>
			<version>1.0.7-SNAPSHOT</version>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>de.mpg.escidoc.services</groupId>
			<artifactId>common_logic</artifactId>
			<version>1.1.4-SNAPSHOT</version>
			<type>ejb</type>
			<scope>provided</scope>
		</dependency>
		<dependency>
	      <groupId>saxon</groupId>
	      <artifactId>saxon-dom</artifactId>
	      <version>8.7</version>
	      <scope>provided</scope>
	    </dependency>
	    
	    <dependency>
	    	<groupId>commons-beanutils</groupId>
	    	<artifactId>commons-beanutils</artifactId>
	    	<version>1.7.0</version>
	    	<scope>provided</scope>
	    </dependency>
	    <dependency>
	    	<groupId>commons-digester</groupId>
	    	<artifactId>commons-digester</artifactId>
	    	<version>1.8</version>
	    	<scope>provided</scope>
	    </dependency>
	    <dependency>
	    	<groupId>richfaces</groupId>
	    	<artifactId>richfaces-api</artifactId>
	    	<version>3.1.4</version>
	    	<scope>provided</scope>
	    </dependency>
	    <dependency>
	    	<groupId>richfaces</groupId>
	    	<artifactId>richfaces-impl</artifactId>
	    	<version>3.1.4</version>
	    	<scope>provided</scope>
	    </dependency>
	    <dependency>
	    	<groupId>richfaces</groupId>
	    	<artifactId>richfaces-ui</artifactId>
	    	<version>3.1.4</version>
	    	<scope>provided</scope>
	    </dependency>
	    <dependency>
	    	<groupId>jsf-facelets</groupId>
	    	<artifactId>jsf-facelets</artifactId>
	    	<version>1.1.13</version>
	    	<scope>provided</scope>
	    </dependency>
	    <dependency>
	    	<groupId>log4j</groupId>
	    	<artifactId>log4j</artifactId>
	    	<version>1.2.14</version>
	    	<scope>provided</scope>
	    </dependency>
	</dependencies>
	<dependencyManagement>
		<dependencies />
	</dependencyManagement>
</project>