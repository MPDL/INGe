	</xs:import>

	<xs:import namespace="http://www.w3.org/XML/1998/namespace"
		schemaLocation="http://www.escidoc.org/schemas/rest/common/0.3/xml.xsd">
	</xs:import>

	<xs:element name="container">
		<xs:annotation>
			<xs:documentation>
				<para>
					Following you will find information which elements
					and attributes are "required", "optional",
					"not-allowed" or will be "discarded" in the input
					XML-stream when creating or updating such an object.
				</para>
				<para>
					In "create" the rule for creating a new object is
					defined.
				</para>
				<para>
					In "update" the rule for updating an object is
					defined.
				</para>
				<para>
					Possible values are: required | optional |
					not-allowed | discarded
				</para>
				<para>
					required: this element or attribute has to be
					delivered
				</para>
				<para>
					optional: this element or attribute can be delivered
					and will be kept
				</para>
				<para>
					not-allowed: this element or attribute is not
					allowed in delivery and will cause an exception
				</para>
				<para>
					discarded: this element or attribute can be
					delivered but will not be used
				</para>
			</xs:documentation>

			<xs:documentation>
				<create>required</create>
				<update>required</update>
			</xs:documentation>
		</xs:annotation>

		<xs:complexType>
			<xs:sequence>
				<xs:element ref="container:properties" minOccurs="0" />
				<xs:element ref="container:resources" minOccurs="0" />
				<xs:element ref="md-records:md-records" minOccurs="0" />
				<xs:element ref="relations:relations" minOccurs="0" />
				<xs:element ref="struct-map:struct-map" minOccurs="0" />
			</xs:sequence>
			<xs:attributeGroup
				ref="common-types:eSciDocResourceIdentityAttributes" />
			<xs:attributeGroup
				ref="common-types:eSciDocRootElementAttributes" />
		</xs:complexType>
		</xs:element>
		<xs:element name="properties">
			<xs:annotation>
				<xs:documentation>
					<create>required</create>
					<update>required</update>
				</xs:documentation>
			</xs:annotation>
			<xs:complexType>
				<xs:sequence>
					<xs:element minOccurs="0"
						ref="prop:creation-date">
						<xs:annotation>
							<xs:documentation>
								<create>discarded</create>
								<update>discarded</update>
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" ref="srel:created-by">
						<xs:annotation>
							<xs:documentation>
								<create>discarded</create>
								<update>discarded</update>
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0"
						ref="prop:public-status">
						<xs:annotation>
							<xs:documentation>
								<create>discarded</create>
								<update>discarded</update>
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" ref="prop:name">
						<xs:annotation>
							<xs:documentation>
								<create>
									discarded (can be set via
									corresponding "escidoc" md-record
									element)
								</create>
								<update>
									discarded (can be updated via update
									of corresponding "escidoc" md-record
									element
								</update>
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" ref="prop:description">
						<xs:annotation>
							<xs:documentation>
								<create>
									discarded (can be set via
									corresponding "escidoc" md-record
									element)
								</create>
								<update>
									discarded (can be updated via update
									of corresponding "escidoc" md-record
									element
								</update>
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" ref="srel:context">
						<xs:annotation>
							<xs:documentation>
								<create>required</create>
								<update>discarded</update>
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0"
						ref="srel:content-model">
						<xs:annotation>
							<xs:documentation>
								<create>required</create>
								<update>discarded</update>
								<comment>not finalized yet</comment>
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" ref="prop:lock-status">
						<xs:annotation>
							<xs:documentation>
								<create>discarded</create>
								<update>discarded</update>
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" ref="prop:lock-date">
						<xs:annotation>
							<xs:documentation>
								<create>discarded</create>
								<update>discarded</update>
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" ref="srel:lock-owner">
						<xs:annotation>
							<xs:documentation>
								<create>discarded</create>
								<update>discarded</update>
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" ref="prop:pid">
						<xs:annotation>
							<xs:documentation>
								<create>optional</create>
								<update>discarded</update>
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" ref="prop:version">
						<xs:annotation>
							<xs:documentation>
								<create>discarded</create>
								<update>discarded</update>
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0"
						ref="prop:latest-version">
						<xs:annotation>
							<xs:documentation>
								<create>discarded</create>
								<update>discarded</update>
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0"
						ref="prop:latest-release">
						<xs:annotation>
							<xs:documentation>
								<create>discarded</create>
								<update>discarded</update>
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element ref="prop:content-model-specific">
						<xs:annotation>
							<xs:documentation>
								<create>required</create>
								<update>required</update>
							</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
				<xs:attributeGroup
					ref="common-types:eSciDocResourceIdentityAttributes" />
				<xs:attributeGroup
					ref="common-types:eSciDocRootElementAttributes" />
			</xs:complexType>
		</xs:element>


		<xs:element name="resources">
			<xs:annotation>
				<xs:documentation>
					<create>discarded</create>
					<update>discarded</update>
				</xs:documentation>
			</xs:annotation>

			<xs:complexType>
				<xs:sequence>
					<xs:element name="members">
						<xs:annotation>
							<xs:documentation>
								<create>discarded</create>
								<update>discarded</update>
							</xs:documentation>
						</xs:annotation>

						<xs:complexType>
							<xs:attributeGroup
								ref="common-types:readOnlyESciDocResourceLinkAttributes">
							</xs:attributeGroup>
						</xs:complexType>
					</xs:element>

					<xs:element name="member-refs">
						<xs:annotation>
							<xs:documentation>
								<create>discarded</create>
								<update>discarded</update>
							</xs:documentation>
						</xs:annotation>

						<xs:complexType>
							<xs:attributeGroup
								ref="common-types:readOnlyESciDocResourceLinkAttributes">
							</xs:attributeGroup>
						</xs:complexType>
					</xs:element>

					<xs:element name="version-history">
						<xs:annotation>
							<xs:documentation>
								<create>discarded</create>
								<update>discarded</update>
							</xs:documentation>
						</xs:annotation>

						<xs:complexType>
							<xs:attributeGroup
								ref="common-types:readOnlyESciDocResourceLinkAttributes">
							</xs:attributeGroup>
						</xs:complexType>
					</xs:element>
				</xs:sequence>

				<xs:attributeGroup
					ref="common-types:readOnlyESciDocResourceLinkAttributes">
				</xs:attributeGroup>

				<xs:attributeGroup
					ref="common-types:eSciDocRootElementAttributes">
				</xs:attributeGroup>
			</xs:complexType>
		</xs:element>
		
</xs:schema>