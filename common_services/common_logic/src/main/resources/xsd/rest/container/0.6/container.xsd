		namespace="http://www.escidoc.de/schemas/metadatarecords/0.4"
		schemaLocation="http://www.escidoc.org/schemas/rest/common/0.4/md-records.xsd">
	</xs:import>

	<xs:import namespace="http://www.escidoc.de/schemas/relations/0.3"
