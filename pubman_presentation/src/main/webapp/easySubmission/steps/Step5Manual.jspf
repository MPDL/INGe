<ui:composition xmlns="http://www.w3.org/1999/xhtml"
	xmlns:f="http://xmlns.jcp.org/jsf/core"
	xmlns:h="http://xmlns.jcp.org/jsf/html"
	xmlns:ui="http://xmlns.jcp.org/jsf/facelets"
	xmlns:p="http://primefaces.org/ui"
	xmlns:pt="http://xmlns.jcp.org/jsf/passthrough">

	<f:subview id="easySubmissionStep3Manual">
		<div class="clear">
			<div class="headerSection headerDistanceFix">
				<ui:include src="../../header/Breadcrumb.jspf" />
				<div id="contentSkipLinkAnchor" class="clear headLine">
					<!-- Headline starts here -->
					<h1>
						<h:outputText value="#{lbl.EasySubmission}" />
					</h1>
					<!-- Headline ends here -->
				</div>
			</div>
			<div class="small_marginLIncl subHeaderSection">
				<div class="contentMenu">
					<!-- content menu starts here -->
					<div class="sub">
						<!-- content menu lower line starts here -->
						<span> <h:outputText
								value="#{lbl.submission_lnkEasySubmission}"
								rendered="#{DepositorWSSessionBean.newSubmission and ContextListSessionBean.depositorContextListSize>0}" />
						</span>
						<h:outputText styleClass="seperator void" />
						<h:commandLink id="lnkNewSubmission"
							title="#{tip.submission_lnkNewSubmission}"
							action="#{CreateItem.newSubmission}" immediate="true"
							onclick="fullItemReloadAjax();">
							<h:outputText value="#{lbl.submission_lnkNewSubmission}"
								rendered="#{DepositorWSSessionBean.newSubmission and ContextListSessionBean.depositorContextListSize>0}" />
						</h:commandLink>
						<h:outputText styleClass="seperator void" />
						<h:commandLink id="lnkNewImport"
							title="#{tip.submission_lnkImport}"
							action="#{EasySubmission.newImport}"
							onclick="fullItemReloadAjax();">
							<h:outputText value="#{lbl.submission_lnkImport}"
								rendered="#{DepositorWSSessionBean.newSubmission and ContextListSessionBean.depositorContextListSize>0}" />
						</h:commandLink>
						<h:outputText styleClass="seperator void" />
						<h:commandLink id="lnkNewImportMultiple"
							title="#{tip.submission_lnkMultipleImport}"
							action="#{MultipleImport.newImport}"
							onclick="fullItemReloadAjax();">
							<h:outputText value="#{lbl.submission_lnkMultipleImport}"
								rendered="#{DepositorWSSessionBean.newSubmission and ContextListSessionBean.depositorContextListSize>0}" />
						</h:commandLink>
						<!-- content menu lower line ends here -->
					</div>
					<!-- content menu ends here -->
				</div>
				<div class="subHeader">
					<!-- Subheadline starts here -->
					<h:outputText
						value="#{lbl.easy_submission_lblCollectionOfItem} #{EasySubmission.contextName}." />
					<!-- Subheadline ends here -->
				</div>
				<div class="subHeader">
					<!-- Subheadline starts here -->
					<h:messages styleClass="singleMessage" errorClass="messageError"
						warnClass="messageWarn" fatalClass="messageFatal"
						infoClass="messageStatus" layout="list" globalOnly="true"
						showDetail="false" showSummary="true" escape="false"
						rendered="#{EasySubmissionSessionBean.numberOfMessages == 1}" />
					<h:panelGroup layout="block"
						styleClass="half_area2_p6 messageArea errorMessageArea"
						rendered="#{EasySubmissionSessionBean.hasErrorMessages and EasySubmissionSessionBean.numberOfMessages != 1}">
						<h2>
							<h:outputText value="#{lbl.warning_lblMessageHeader}" />
						</h2>
						<h:messages errorClass="messageError" warnClass="messageWarn"
							fatalClass="messageFatal" infoClass="messageStatus" layout="list"
							globalOnly="true" showDetail="false" showSummary="true" escape="false"
							rendered="#{EasySubmissionSessionBean.hasMessages}" />
					</h:panelGroup>
					<h:panelGroup layout="block"
						styleClass="half_area2_p6 messageArea infoMessageArea"
						rendered="#{EasySubmissionSessionBean.hasMessages and !EasySubmissionSessionBean.hasErrorMessages  and EasySubmissionSessionBean.numberOfMessages != 1}">
						<h2>
							<h:outputText value="#{lbl.info_lblMessageHeader}" />
						</h2>
						<h:messages errorClass="messageError" warnClass="messageWarn"
							fatalClass="messageFatal" infoClass="messageStatus" layout="list"
							globalOnly="true" showDetail="false" showSummary="true" escape="false"
							rendered="#{EasySubmissionSessionBean.hasMessages}" />
					</h:panelGroup>
					<!-- Subheadline ends here -->
				</div>
			</div>
		</div>
		<div id="ImgFullItem">
			<div id="ImgFullItemLoad" class="noDisplay" style="position: fixed;"></div>
		</div>
		<div id="fullItem" class="full_area0">
			<div class="full_area0 fullItem">
				<h:panelGroup layout="block"
					styleClass="full_area0 easySubmissionHeader">
					<span class="small_area0 endline"> &#160; </span>
					<span class="seperator"></span>
					<span class="endline huge_area0_p7 step messageSuccess"> <b>
							<span class="free_area0"> <h:outputText
									value="#{lbl.easy_submission_lblStep}" /> 1
						</span>
					</b> <h:panelGroup
							styleClass="free_area0 xTiny_marginLExcl min_imgBtn submitSuccessful">&#160;</h:panelGroup>
					</span>
					<span class="seperator"></span>
					<span class="endline huge_area0_p7 step messageSuccess"> <b>
							<span class="free_area0"> <h:outputText
									value="#{lbl.easy_submission_lblStep}" /> 2
						</span>
					</b> <h:panelGroup
							styleClass="free_area0 xTiny_marginLExcl min_imgBtn submitSuccessful">&#160;</h:panelGroup>
					</span>
					<span class="seperator"></span>
					<span class="endline huge_area0_p7 step"> <b class="actual">
							<h:outputText value="#{lbl.easy_submission_lblStep}" /> 3
					</b>
					</span>
				</h:panelGroup>
				<h:panelGroup layout="block" styleClass="full_area0 itemBlock"
					rendered="#{!(genre.details_display == 'false' and genre.details_form_id == 'easy-submission' || genre.details_display == 'false' and genre.details_form_id == 'all')}">
					<h3 class="xLarge_area0_p8 endline blockHeader">
						<h:outputText value="#{genre.details_label}"
							converter="GenreLabelConverter" />
					</h3>
					<span class="seperator"></span>
					<div class="free_area0 itemBlockContent endline">
						<!-- Language of Publication -->
						<h:panelGroup layout="block"
							styleClass="free_area0 endline itemLine noTopBorder languageArea"
							rendered="#{!(genre.details_item_language_display == 'false' and genre.details_item_language_form_id == 'easy-submission' || genre.details_item_language_display == 'false' and genre.details_item_language_form_id == 'all')}">
							<b class="xLarge_area0_p8 endline labelLine clear"> &#160;<h:outputText
									styleClass="mandatory" title="#{msg.mandatoryField}" value="*"
									rendered="#{genre.details_item_language_item_language_optional != 'true'}" />
								<h:outputText
									value="#{genre.details_item_language_item_language_label}"
									converter="GenreLabelConverter" /><span class="noDisplay">:
							</span>
							</b>
							<span class="xHuge_area0 xTiny_marginLExcl endline"> <h:inputText
									styleClass="large_txtInput tiny_marginRIncl languageSuggest"
									id="inpLanguageOfPublication"
									value="#{EasySubmission.publicationLanguage}" /> <h:inputText
									styleClass="xDouble_txtInput tiny_marginRIncl languageText readonlyInput"
									value="#{EasySubmission.alternativeLanguageName}"
									readonly="true" />
							</span>
						</h:panelGroup>
						<!-- Dates -->
						<h:panelGroup layout="block"
							styleClass="free_area0 endline itemLine noTopBorder"
							rendered="#{!(genre.details_item_details_dates_display == 'false' and genre.details_item_details_dates_form_id == 'easy-submission' || genre.details_item_details_dates_display == 'false' and genre.details_item_details_dates_form_id == 'all')}">
							<b class="xLarge_area0_p8 endline labelLine clear"> &#160;<h:outputText
									styleClass="mandatory" title="#{msg.mandatoryField}" value="*"
									rendered="#{genre.details_item_details_dates_optional != 'true'}" /><span
								class="noDisplay"> </span>
							</b>
							<span class="xHuge_area0 xTiny_marginLExcl endline datesGroup">
								<h:panelGroup
									rendered="#{!(genre.details_item_details_dates_item_date_issued_display == 'false' and genre.details_item_details_dates_item_date_issued_form_id == 'easy-submission' || genre.details_item_details_dates_item_date_issued_display == 'false' and genre.details_item_details_dates_item_date_issued_form_id == 'all')}">
									<span class="xLarge_area0 tiny_marginRIncl"> <h:outputLabel
											id="lblDatePublishedInPrint" styleClass="xLarge_label"
											for="txtDatePublishedInPrint"
											value="#{genre.details_item_details_dates_item_date_issued_label}"
											converter="GenreLabelConverter" /> <h:inputText
											styleClass="large_txtInput" id="txtDatePublishedInPrint"
											value="#{ItemControllerSessionBean.currentPubItem.metadata.datePublishedInPrint}"
											pt:placeholder="#{lbl.EditItem_DateFormatPlaceholder}"
											title="#{tip.DateFormatDescription}" />
									</span>
								</h:panelGroup> <h:panelGroup
									rendered="#{!(genre.details_item_details_dates_item_published_online_display == 'false' and genre.details_item_details_dates_item_published_online_form_id == 'easy-submission' || genre.details_item_details_dates_item_published_online_display == 'false' and genre.details_item_details_dates_item_published_online_form_id == 'all')}">
									<span class="xLarge_area0 tiny_marginRIncl"> <h:outputLabel
											id="lblDatePublishedOnline" styleClass="xLarge_label"
											for="txtDatePublishedOnline"
											value="#{genre.details_item_details_dates_item_published_online_label}"
											converter="GenreLabelConverter" /> <h:inputText
											styleClass="large_txtInput" id="txtDatePublishedOnline"
											value="#{ItemControllerSessionBean.currentPubItem.metadata.datePublishedOnline}"
											pt:placeholder="#{lbl.EditItem_DateFormatPlaceholder}"
											title="#{tip.DateFormatDescription}" />
									</span>
								</h:panelGroup> <h:panelGroup
									rendered="#{!(genre.details_item_details_dates_item_date_accepted_display == 'false' and genre.details_item_details_dates_item_date_accepted_form_id == 'easy-submission' || genre.details_item_details_dates_item_date_accepted_display == 'false' and genre.details_item_details_dates_item_date_accepted_form_id == 'all')}">
									<span class="xLarge_area0 tiny_marginRIncl"> <h:outputLabel
											id="lblDateAccepted" styleClass="xLarge_label"
											for="txtDateAccepted"
											value="#{genre.details_item_details_dates_item_date_accepted_label}"
											converter="GenreLabelConverter" /> <h:inputText
											styleClass="large_txtInput" id="txtDateAccepted"
											value="#{ItemControllerSessionBean.currentPubItem.metadata.dateAccepted}"
											pt:placeholder="#{lbl.EditItem_DateFormatPlaceholder}"
											title="#{tip.DateFormatDescription}" />
									</span>
								</h:panelGroup> <h:panelGroup
									rendered="#{!(genre.details_item_details_dates_item_date_submitted_display == 'false' and genre.details_item_details_dates_item_date_submitted_form_id == 'easy-submission' || genre.details_item_details_dates_item_date_submitted_display == 'false' and genre.details_item_details_dates_item_date_submitted_form_id == 'all')}">
									<span class="xLarge_area0 tiny_marginRIncl"> <h:outputLabel
											id="lblDateSubmitted" styleClass="xLarge_label"
											for="txtDateSubmitted"
											value="#{genre.details_item_details_dates_item_date_submitted_label}"
											converter="GenreLabelConverter" /> <h:inputText
											styleClass="large_txtInput" id="txtDateSubmitted"
											value="#{ItemControllerSessionBean.currentPubItem.metadata.dateSubmitted}"
											pt:placeholder="#{lbl.EditItem_DateFormatPlaceholder}"
											title="#{tip.DateFormatDescription}" />
									</span>
								</h:panelGroup> <h:panelGroup
									rendered="#{!(genre.details_item_details_dates_item_date_modified_display == 'false' and genre.details_item_details_dates_item_date_modified_form_id == 'easy-submission' || genre.details_item_details_dates_item_date_modified_display == 'false' and genre.details_item_details_dates_item_date_modified_form_id == 'all')}">
									<span class="xLarge_area0 tiny_marginRIncl"> <h:outputLabel
											id="lblDateModified" styleClass="xLarge_label"
											for="txtDateModified"
											value="#{genre.details_item_details_dates_item_date_modified_label}"
											converter="GenreLabelConverter" /> <h:inputText
											styleClass="large_txtInput" id="txtDateModified"
											value="#{ItemControllerSessionBean.currentPubItem.metadata.dateModified}"
											pt:placeholder="#{lbl.EditItem_DateFormatPlaceholder}"
											title="#{tip.DateFormatDescription}" />
									</span>
								</h:panelGroup> <h:panelGroup
									rendered="#{!(genre.details_item_details_dates_item_date_created_display == 'false' and genre.details_item_details_dates_item_date_created_form_id == 'easy-submission' || genre.details_item_details_dates_item_date_created_display == 'false' and genre.details_item_details_dates_item_date_created_form_id == 'all')}">
									<span class="xLarge_area0 tiny_marginRIncl"> <h:outputLabel
											id="lblDateCreated" styleClass="xLarge_label"
											for="txtDateCreated"
											value="#{genre.details_item_details_dates_item_date_created_label}"
											converter="GenreLabelConverter" /> <h:inputText
											styleClass="large_txtInput" id="txtDateCreated"
											value="#{ItemControllerSessionBean.currentPubItem.metadata.dateCreated}"
											pt:placeholder="#{lbl.EditItem_DateFormatPlaceholder}"
											title="#{tip.DateFormatDescription}" />
									</span>
								</h:panelGroup> <a
								class="large_area0 tiny_marginRIncl toLabelInputBottom showMoreDates"><h:outputText
										value="#{lbl.adv_search_btShowMore}" /></a>
							</span>
						</h:panelGroup>
						<!-- Number of Pages -->
						<h:panelGroup layout="block"
							styleClass="free_area0 endline itemLine noTopBorder"
							rendered="#{!(genre.details_total_number_of_pages_display == 'false' and genre.details_total_number_of_pages_form_id == 'easy-submission' || genre.details_total_number_of_pages_display == 'false' and genre.details_total_number_of_pages_form_id == 'all')}">
							<h:panelGroup
								rendered="#{!(genre.details_total_number_of_pages_item_total_number_of_pages_display == 'false' and genre.details_total_number_of_pages_item_total_number_of_pages_form_id == 'easy-submission' || genre.details_total_number_of_pages_item_total_number_of_pages_display == 'false' and genre.details_total_number_of_pages_item_total_number_of_pages_form_id == 'all')}">
								<b class="xLarge_area0_p8 endline labelLine clear"> &#160;<h:outputText
										styleClass="mandatory" title="#{msg.mandatoryField}" value="*"
										rendered="#{genre.details_total_number_of_pages_item_total_number_of_pages_optional != 'true'}" /><span
									class="noDisplay"> </span>
								</b>
								<span class="xHuge_area0 xTiny_marginLExcl endline"> <span
									class="large_area0 tiny_marginRIncl"> <h:outputLabel
											id="lblTotalNoOfPages" styleClass="large_label"
											for="txtTotalNoOfPages"
											value="#{genre.details_total_number_of_pages_item_total_number_of_pages_label}"
											converter="GenreLabelConverter" /> <h:inputText
											styleClass="large_txtInput" id="txtTotalNoOfPages"
											value="#{ItemControllerSessionBean.currentPubItem.metadata.totalNumberOfPages}" />
								</span>
								</span>
							</h:panelGroup>
						</h:panelGroup>
						<!-- Publisher, Place -->
						<h:panelGroup layout="block"
							styleClass="free_area0 endline itemLine noTopBorder"
							rendered="#{!(genre.details_publishing_info_display == 'false' and genre.details_publishing_info_form_id == 'easy-submission' || genre.details_publishing_info_display == 'false' and genre.details_publishing_info_form_id == 'all')}">
							<b class="xLarge_area0_p8 endline labelLine clear"> &#160;<h:outputText
									styleClass="mandatory" title="#{msg.mandatoryField}" value="*"
									rendered="#{genre.details_publishing_info_publishing_info_publisher_optional != 'true' || genre.details_publishing_info_publishing_info_place_optional != 'true'}" /><span
								class="noDisplay"> </span>
							</b>
							<span class="xHuge_area0 xTiny_marginLExcl endline"> <h:panelGroup
									styleClass="xDouble_area0 tiny_marginRIncl"
									rendered="#{!(genre.details_publishing_info_publishing_info_publisher_display == 'false' and genre.details_publishing_info_publishing_info_publisher_form_id == 'easy-submission' || genre.details_publishing_info_publishing_info_publisher_display == 'false' and genre.details_publishing_info_publishing_info_publisher_form_id == 'all')}">
									<h:outputLabel styleClass="xDouble_label" id="lblPublisher"
										for="txtaPublisher"
										value="#{genre.details_publishing_info_publishing_info_publisher_label}"
										converter="GenreLabelConverter" />
									<h:inputTextarea styleClass="xDouble_txtArea inputTxtArea"
										id="txtaPublisher"
										value="#{ItemControllerSessionBean.currentPubItem.metadata.publishingInfo.publisher}"
										cols="15" rows="3" />
								</h:panelGroup> <h:panelGroup styleClass="large_area0 tiny_marginRIncl"
									rendered="#{!(genre.details_publishing_info_publishing_info_place_display == 'false' and genre.details_publishing_info_publishing_info_place_form_id == 'easy-submission' || genre.details_publishing_info_publishing_info_place_display == 'false' and genre.details_publishing_info_publishing_info_place_form_id == 'all')}">
									<h:outputLabel styleClass="large_label" id="lblPlace"
										for="txtPlace"
										value="#{genre.details_publishing_info_publishing_info_place_label}"
										converter="GenreLabelConverter" />
									<h:inputText styleClass="large_txtInput" id="txtPlace"
										value="#{ItemControllerSessionBean.currentPubItem.metadata.publishingInfo.place}" />
								</h:panelGroup>
							</span>
						</h:panelGroup>
						<!-- Identifier -->
						<h:panelGroup layout="block"
							rendered="#{!(genre.details_identifiers_display == 'false' and genre.details_identifiers_form_id == 'easy-submission' || genre.details_identifiers_display == 'false' and genre.details_identifiers_form_id == 'all')}">
							<ui:repeat var="identifier"
								value="#{EasySubmission.identifierCollection.identifierManager.objectDM}"
								varStatus="status">
								<h:panelGroup layout="block"
									styleClass="free_area0 endline itemLine noTopBorder">
									<b class="xLarge_area0_p8 endline labelLine clear"> &#160;<h:outputText
											styleClass="mandatory" title="#{msg.mandatoryField}"
											value="*"
											rendered="#{genre.details_identifiers_optional != 'true' and (genre.details_identifiers_item_identifier_type_optional != 'true' || genre.details_identifiers_item_identifier_value_optional != 'true')}" />
										<h:outputText value="#{genre.details_identifiers_label}"
											converter="GenreLabelConverter"
											rendered="#{EasySubmission.identifierCollection.identifierManager.objectDM.rowIndex == 0}" /><span
										class="noDisplay">: </span>
									</b>
									<span class="xHuge_area0 xTiny_marginLExcl endline"> <h:panelGroup
											rendered="#{!(genre.details_identifiers_item_identifier_type_display == 'false' and genre.details_identifiers_item_identifier_type_form_id == 'easy-submission' || genre.details_identifiers_item_identifier_type_display == 'false' and genre.details_identifiers_item_identifier_type_form_id == 'all')}">
											<span class="large_area0 largeFormPush"> <h:outputLabel
													id="lblDetailsIdentifiersItemIdentifierType"
													styleClass="large_label"
													value="#{genre.details_identifiers_item_identifier_type_label}"
													converter="GenreLabelConverter" /> <h:panelGroup
													layout="block"
													styleClass="large_area1 endline selectContainer">
													<h:panelGroup layout="block" styleClass="large_area0">
														<h:panelGroup styleClass="large_area0 selectionBox">&#160;</h:panelGroup>
														<h:panelGroup layout="block"
															styleClass="min_imgArea selectboxIcon">&#160;</h:panelGroup>
													</h:panelGroup>
													<h:selectOneMenu id="selTypeString" styleClass="replace"
														onfocus="updateSelectionBox(this);"
														value="#{identifier.typeString}"
														onchange="updateSelectionBox(this);">
														<f:selectItems id="selIdentifierTypes"
															value="#{EasySubmission.identifierCollection.identifierTypes}" />
													</h:selectOneMenu>
												</h:panelGroup> <!--	<h:selectOneMenu id="selTypeString" styleClass="large_select replace" value="#{identifier.typeString}">
											<f:selectItems id="selIdentifierTypes" value="#{EasySubmission.identifierCollection.identifierTypes}"/>
										</h:selectOneMenu>	-->
											</span>
										</h:panelGroup> <h:panelGroup
											rendered="#{!(genre.details_identifiers_item_identifier_value_display == 'false' and genre.details_identifiers_item_identifier_value_form_id == 'easy-submission' || genre.details_identifiers_item_identifier_value_display == 'false' and genre.details_identifiers_item_identifier_value_form_id == 'all')}">
											<h:outputLabel id="lblDetailsIdentifiersItemIdentifierValue"
												styleClass="xDouble_label"
												value="#{genre.details_identifiers_item_identifier_value_label}"
												converter="GenreLabelConverter" />
											<h:inputText styleClass="xDouble_txtInput"
												value="#{identifier.id}" />
										</h:panelGroup>
									</span>
									<span class="large_area0_p8 lineToolSection"> <h:commandButton
											id="btnAddObject" styleClass="min_imgBtn groupTool add"
											value=" " title="#{tip.easy_submission_addID}"
											action="#{EasySubmission.identifierCollection.identifierManager.addObject}"
											rendered="#{genre.details_identifiers_repeatable != 'false'}" />
										<h:commandButton id="btnRemoveObject"
											styleClass="min_imgBtn groupTool remove" value=" "
											title="#{tip.easy_submission_removeID}"
											action="#{EasySubmission.identifierCollection.identifierManager.removeObject}"
											disabled="#{EasySubmission.identifierCollection.identifierManager.size == 1}"
											rendered="#{genre.details_identifiers_repeatable != 'false'}" />
									</span>
								</h:panelGroup>
							</ui:repeat>
						</h:panelGroup>
						<!-- Degree type -->
						<h:panelGroup layout="block"
							styleClass="free_area0 endline itemLine noTopBorder"
							rendered="#{!(genre.details_item_degree_display == 'false' and genre.details_item_degree_form_id == 'easy-submission' || genre.details_item_degree_display == 'false' and genre.details_item_degree_form_id == 'all')}">
							<h:panelGroup
								rendered="#{!(genre.details_item_degree_item_degree_display == 'false' and genre.details_item_degree_item_degree_form_id == 'easy-submission' || genre.details_item_degree_item_degree_display == 'false' and genre.details_item_degree_item_degree_form_id == 'all')}">
								<b class="xLarge_area0_p8 endline labelLine clear"> &#160;<h:outputText
										styleClass="mandatory" title="#{msg.mandatoryField}" value="*"
										rendered="#{genre.details_item_degree_item_degree_optional != 'true'}" />
									<h:outputText value="#{genre.details_item_degree_label}"
										converter="GenreLabelConverter" /><span class="noDisplay">
								</span>
								</b>
								<span class="xHuge_area0 xTiny_marginLExcl endline"> <span
									class="large_area0 tiny_marginRIncl"> <h:outputLabel
											styleClass="large_label" for="cboDegreeType"
											id="lblDegreeType"
											value="#{genre.details_item_degree_item_degree_label}"
											converter="GenreLabelConverter" /> <h:panelGroup
											layout="block"
